<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-4.2.xsd"
	default-lazy-init="true">
   <bean id="dbh_content_page" class="com.menksoft.cms.freemarker.Cms.CmsContentPage"/>
   <bean id="dbh_channel" class="com.menksoft.cms.freemarker.Cms.CmsChannel"/>
   <bean id="dbh_content_list" class="com.menksoft.cms.freemarker.Cms.CmsContentList"/>
   <bean id="dbh_slideshow" class="com.menksoft.cms.freemarker.Cms.CmsSlideShow"/>
   <bean id="dbh_channel_list" class="com.menksoft.cms.freemarker.Cms.CmsChannelList"/>
   <bean id="dbh_content" class="com.menksoft.cms.freemarker.Cms.CmsContent"/>
  
   <bean id="dbh_website" class="com.menksoft.cms.freemarker.Cms.CmsWebSite"/>
   <bean id="dbh_links" class="com.menksoft.cms.freemarker.Cms.CmsLinks"/>
   <bean id="dbh_search" class="com.menksoft.cms.freemarker.Cms.CmsSearch"/>
   <bean id="dbh_search_wap" class="com.menksoft.cms.freemarker.Cms.CmsSearchWap"/>
  
   <bean id="dbh_attachment" class="com.menksoft.cms.freemarker.Cms.CmsAttachment"/>
   <bean id="dbh_servlet" class="com.menksoft.cms.freemarker.Cms.CmsServlet"/>
   <bean id="dbh_member" class="com.menksoft.cms.freemarker.Cms.CmsMember"/>
   <bean id="dbh_member_paylist" class="com.menksoft.cms.freemarker.Cms.CmsMemberPayList"/>
   <bean id="dbh_member_consumelist" class="com.menksoft.cms.freemarker.Cms.CmsMemberConsumeList"/>
   <bean id="dbh_member_viewlist" class="com.menksoft.cms.freemarker.Cms.CmsMemberViewList"/>
   <bean id="dbh_member_favoritelist" class="com.menksoft.cms.freemarker.Cms.CmsMemberFavoriteList"/>
   <bean id="dbh_member_type" class="com.menksoft.cms.freemarker.Cms.CmsMemberType"/>
   <bean id="dbh_bot" class="com.menksoft.cms.freemarker.Cms.CmsBot"/>
   
   <bean id="dbh_project" class="com.menksoft.cms.freemarker.Cms.CmsProject_M"/>
   <bean id="freeMarkerConfigurer" class="org.springframework.web.servlet.view.freemarker.FreeMarkerConfigurer">
				<property name="freemarkerVariables">
					<map>
						<entry key="dbh_content_page" value-ref="dbh_content_page"/>
						<entry key="dbh_channel_list" value-ref="dbh_channel_list"/>
						<entry key="dbh_channel" value-ref="dbh_channel"/>
						<entry key="dbh_content_list" value-ref="dbh_content_list"/>
						<entry key="dbh_slideshow" value-ref="dbh_slideshow"/>
						<entry key="dbh_content" value-ref="dbh_content"/>
						
						<entry key="dbh_website" value-ref="dbh_website"/>
						<entry key="dbh_links" value-ref="dbh_links"/>
						<entry key="dbh_search" value-ref="dbh_search"/>
						<entry key="dbh_search_wap" value-ref="dbh_search_wap"/>
						
						<entry key="dbh_attachment" value-ref="dbh_attachment"/>
						<entry key="dbh_servlet" value-ref="dbh_servlet"/>
						<entry key="dbh_member" value-ref="dbh_member"/>
						<entry key="dbh_member_paylist" value-ref="dbh_member_paylist"/>
						<entry key="dbh_member_consumelist" value-ref="dbh_member_consumelist"/>
						<entry key="dbh_member_viewlist" value-ref="dbh_member_viewlist"/>
						<entry key="dbh_member_favoritelist" value-ref="dbh_member_favoritelist"/>
						<entry key="dbh_member_type" value-ref="dbh_member_type"/>
						<entry key="dbh_project" value-ref="dbh_project"/>
					</map>
				</property>
				 <property name="templateLoaderPath" value="/WEB-INF/views/template/"/>  
				<property name="freemarkerSettings">
					<props>
						<prop key="tag_syntax">square_bracket</prop>
						<prop key="template_update_delay">10</prop>
						<prop key="defaultEncoding">UTF-8</prop>
						<prop key="url_escaping_charset">UTF-8</prop>
						<prop key="locale">zh_CN</prop>
						<prop key="boolean_format">true,false</prop>
						<prop key="datetime_format">yyyy-MM-dd HH:mm:ss</prop>
						<prop key="date_format">yyyy-MM-dd</prop>
						<prop key="time_format">HH:mm:ss</prop>
						<prop key="number_format">0.######</prop>
						<prop key="whitespace_stripping">true</prop>
						<prop key="classic_compatible">true</prop> 
					</props>
				</property>
			</bean>
			
	<bean id="freeMarkerConfiguration" class="org.springframework.ui.freemarker.FreeMarkerConfigurationFactoryBean">
		<property name="templateLoaderPath" value="/WEB-INF/views/template/"/>  
	</bean>
	
	  <bean id="MakePageService" class="com.menksoft.cms.freemarker.service.impl.MakePageServiceImpl"> 
        <property name="freeMarkerConfigurer" ref="freeMarkerConfigurer" /> 
        <property name="freeMarkerConfiguration" ref="freeMarkerConfiguration" />
    </bean> 
  

    
    
</beans>
